{
  "name": "poppy-ai-canvas-backend",
  "version": "1.0.0",
  "description": "Backend API for Poppy AI Canvas with PostgreSQL, AI integration, and real-time collaboration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "db:migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.4",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "prisma": "^5.9.1",
    "sharp": "^0.33.2",
    "socket.io": "^4.8.1",
    "stripe": "^14.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.3.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "ai",
    "canvas",
    "collaboration",
    "openai",
    "postgresql",
    "websocket"
  ],
  "author": "Poppy AI Canvas Team",
  "license": "MIT",
  "type": "commonjs",
  "types": "./dist/server.d.ts"
}
